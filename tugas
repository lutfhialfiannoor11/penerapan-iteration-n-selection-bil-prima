#include <iostream>
using namespace std;
int main ()
{
	int angkaAwal = 10;
	int angkaAkhir = 250;
	
	for (int i = angkaAwal ; i <= angkaAkhir ; i++) 
	{
		int counterPrima = 0;
		for (int j = 1 ; j <= i ; j++) {
			if ( i % j == 0 ) {
				counterPrima++;
			}
		}
		if (counterPrima == 2) {
			cout << i << " adalah bilangan PRIMA " << endl;
		}
	}
	return 0;
}
